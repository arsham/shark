{
  "Iferr": {
    "prefix": ["if"],
    "body": [
      "if err != nil {",
      "    return ${1:nil}, errors.Wrap${2:f}(err, \"${3}\")",
      "}"
    ],
    "description": "If error then return"
  },
  "TestCases": {
    "prefix": ["tcs"],
    "body": [
      "tcs := map[string]struct {",
      "    ${1:output}",
      "}{",
      "    // Test cases here",
      "}",
      "for name, tc := range tcs {",
      "    tc := tc",
      "    t.Run(name, func(t *testing.T) {",
      "        ${2:code}",
      "    })",
      "}"
    ]
  },
  "go-cmp diff": {
    "prefix": ["gocmp"],
    "body": [
      "if diff := cmp.Diff(${1:want}, ${2:got}); diff != \"\" {",
      "    t.Errorf(\"(-want +got):\\n%s\", diff)",
      "}"
    ]
  }
}
